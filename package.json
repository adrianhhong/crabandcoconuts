{
  "name": "skull",
  "version": "1.0.0",
  "description": "Skull board game",
  "main": "app.ts",
  "scripts": {
    "serve:client": "vue-cli-service serve",
    "serve:server": "nodemon --config nodemon.json ./app.ts",
    "lint:client": "vue-cli-service lint",
    "lint:server": "eslint",
    "start": "node dist/app.js",
    "start:debug": "nodemon --config nodemon.json --inspect-brk ./src/app.ts",
    "build": "vue-cli-service build && tsc",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "16.2.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/",
      "dist/",
      "coverage/"
    ],
    "watch": [
      "src/"
    ],
    "execMap": {
      "ts": "node -r ts-node/register"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
  },
  "author": "Adrian Hong",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "v-clipboard": "^2.2.3",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue-socket.io-extended": "^4.2.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.6.2",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.7",
    "prettier": "2.3.1",
    "sass": "~1.32.12",
    "sass-loader": "^10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "vue-cli-plugin-vuetify": "~2.4.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  }
}
